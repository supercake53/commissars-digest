{
  "expo": {
    "name": "Commissar's Digest",
    "slug": "commissars-digest",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.commissarsdigest.app"
    },
    "android": {
      "package": "com.commissarsdigest.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-system-ui",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "commissars-digest"
      }
    },
    "platforms": ["ios", "android", "web"]
  }
}
